{
  "name": "generator-moped",
  "version": "0.0.21",
  "description": "",
  "files": [
    "generators"
  ],
  "keywords": [
    "yeoman-generator"
  ],
  "dependencies": {
    "@types/node": "^8.0.33",
    "chalk": "^2.3.0",
    "mkdirp": "^0.5.1",
    "yeoman-generator": "^1.0.0"
  },
  "scripts": {
    "prepublish": "node ../../prebuild generator-moped&& tsc -p tsconfig.build.json && node ../../prepare generator-moped",
    "pretest": "node restore-postinstall",
    "test": "cd ../generator-moped-output-demo && yarn db down all && cd ../ && yo moped generator-moped-output-demo --force",
    "posttest": "node clear-postinstall",
    "dependency-check": "dependency-check package.json \"./generators/app/index.js\" --quiet && dependency-check ./package.json \"./generators/app/index.js\" --quiet --unused --no-dev --ignore-module \"@types/node\" --ignore-module \"@types/chalk\" --ignore-module \"@types/fs-extra\" --ignore-module \"@types/node\" --ignore-module \"@types/yeoman-generator\" --ignore-module \"@types/yeoman-test\""
  },
  "repository": "https://github.com/mopedjs/moped/tree/master/packages/generator-moped",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "@moped/target": "yeoman",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "devDependencies": {
    "@authentication/cookie": "^0.0.4",
    "@moped/db-pg": "^0.0.11",
    "@moped/db-pg-migrations": "^0.0.12",
    "@moped/sentry": "^0.0.4",
    "@moped/serve-assets": "^0.0.6",
    "@moped/server-side-render": "^0.0.5",
    "@moped/sql": "^0.0.5",
    "@moped/status-code": "^0.0.4",
    "@types/chalk": "^2.2.0",
    "@types/fs-extra": "^5.0.0",
    "@types/node": "^8.0.33",
    "@types/yeoman-generator": "^2.0.0",
    "@types/yeoman-test": "^1.7.3",
    "fs-extra": "^5.0.0",
    "moped": "^0.7.12",
    "yeoman-test": "^1.7.0"
  },
  "dependency-check-entries": [
    "./generators/app/index.js"
  ]
}
